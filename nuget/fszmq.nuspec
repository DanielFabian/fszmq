<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>@project@</id>
    <version>@build.number@</version>
    <authors>@authors@</authors>
    <owners>Henrik Feldt,Paulmichael Blasucci</owners>
    <licenseUrl>http://github.com/pblasucci/fszmq/blob/master/LICENSE.txt</licenseUrl>
    <projectUrl>http://pblasucci.github.io/fszmq</projectUrl>
    <iconUrl>https://raw.github.com/fsharp/fszmq/master/docs/files/img/zmq_logo.png</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <summary>@summary@</summary>
    <description>@description@</description>
    <releaseNotes>@releaseNotes@</releaseNotes>
    <copyright>&#169; Paulmichael Blasucci. All rights reserved.</copyright>
    <tags>@tags@</tags>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="FSharp.Core" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="fszmq.dll"   target="lib\net45\fszmq.dll"  />
    <file src="fszmq.xml"   target="lib\net45\fszmq.xml"  />
    <file src="libzmq.dll"  target="content\libzmq.dll"   />
    <file src="Install.ps1" target="tools\Install.ps1"    />
  </files>
</package>
